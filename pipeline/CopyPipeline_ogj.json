{
	"name": "CopyPipeline_ogj",
	"properties": {
		"activities": [
			{
				"name": "ForEach_ogj",
				"type": "ForEach",
				"dependsOn": [],
				"userProperties": [],
				"typeProperties": {
					"items": {
						"value": "@pipeline().parameters.cw_items",
						"type": "Expression"
					},
					"activities": [
						{
							"name": "Copy_ogj",
							"type": "Copy",
							"dependsOn": [],
							"policy": {
								"timeout": "7.00:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [
								{
									"name": "Source",
									"value": "dbo.@{item().source.table}"
								},
								{
									"name": "Destination",
									"value": "output//@{item().destination.fileName}"
								}
							],
							"typeProperties": {
								"source": {
									"type": "AzureSqlSource",
									"partitionOption": "None"
								},
								"sink": {
									"type": "DelimitedTextSink",
									"storeSettings": {
										"type": "AzureBlobStorageWriteSettings"
									},
									"formatSettings": {
										"type": "DelimitedTextWriteSettings",
										"quoteAllText": true,
										"fileExtension": ".txt"
									}
								},
								"enableStaging": false,
								"validateDataConsistency": false
							},
							"inputs": [
								{
									"referenceName": "SourceDataset_ogj",
									"type": "DatasetReference",
									"parameters": {
										"cw_table": "@item().source.table"
									}
								}
							],
							"outputs": [
								{
									"referenceName": "DestinationDataset_ogj",
									"type": "DatasetReference",
									"parameters": {
										"cw_fileName": "@item().destination.fileName"
									}
								}
							]
						}
					]
				}
			}
		],
		"parameters": {
			"cw_items": {
				"type": "Array",
				"defaultValue": [
					{
						"source": {
							"table": "certification_ID"
						},
						"destination": {
							"fileName": "dbocertification_ID"
						}
					},
					{
						"source": {
							"table": "Course"
						},
						"destination": {
							"fileName": "dboCourse"
						}
					},
					{
						"source": {
							"table": "Employers_Users"
						},
						"destination": {
							"fileName": "dboEmployers_Users"
						}
					},
					{
						"source": {
							"table": "Ongoing_Training"
						},
						"destination": {
							"fileName": "dboOngoing_Training"
						}
					},
					{
						"source": {
							"table": "Pictures"
						},
						"destination": {
							"fileName": "dboPictures"
						}
					},
					{
						"source": {
							"table": "Platform"
						},
						"destination": {
							"fileName": "dboPlatform"
						}
					},
					{
						"source": {
							"table": "Review"
						},
						"destination": {
							"fileName": "dboReview"
						}
					},
					{
						"source": {
							"table": "sqlite_sequence"
						},
						"destination": {
							"fileName": "dbosqlite_sequence"
						}
					}
				]
			}
		},
		"annotations": []
	}
}